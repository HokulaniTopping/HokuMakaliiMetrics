generator client {
  provider      = "prisma-client-js"
  binaryTargets = ["native"]
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Valley {
  unique_sample_number Int     @id @unique(map: "unique_sample_number_UNIQUE") @default(autoincrement())
  sample_id            String? @db.Text
  TEC                  Float?
  pH                   Float?
  EC                   Float?
  Sulfur               BigInt?
  Phosphorus           BigInt?
  Olsen_P              BigInt? @map("Olsen P")
  Calcium              BigInt?
  Magnesium            BigInt?
  Potassium            BigInt?
  Sodium               BigInt?
  Boron                Float?
  Iron                 BigInt?
  Manganese            BigInt?
  Copper               Float?
  Zinc                 Float?
  Aluminum             BigInt?
  Total_Carbon__       Float?  @map("Total Carbon %")
  Total_Nitrogen__     Float?  @map("Total Nitrogen %")
  C_N_Ratio            Float?  @map("C/N Ratio")
}

